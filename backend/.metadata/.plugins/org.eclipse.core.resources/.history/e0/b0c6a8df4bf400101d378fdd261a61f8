package com.oas.auctionservice.service;

import com.oas.auctionservice.model.Auction;

import java.util.List;

public interface AuctionService {

    Auction createAuction(Auction auction);

    Auction getAuctionById(String auctionId);

    List<Auction> getAllAuctions();

    Auction updateAuction(String auctionId, Auction auction);

    void closeAuction(String auctionId);
}
